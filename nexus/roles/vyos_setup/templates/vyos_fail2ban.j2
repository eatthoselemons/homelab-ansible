[DEFAULT]
bantime = 3600
findtime = 600
maxretry = 3
destemail = root@localhost
sender = fail2ban@localhost
action = %(action_)s

[sshd]
enabled = true
port = ssh
filter = sshd
logpath = /var/log/auth.log
maxretry = 3
bantime = 3600

[vyos-web]
enabled = true
port = https
filter = vyos-web
logpath = /var/log/vyos/vyos-web.log
maxretry = 3
bantime = 3600 